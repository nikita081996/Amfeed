{
  "expo": {
    "name": "Amfeed",
    "description": "Feed you Need",
    "slug": "Amfeed",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "www.hunchdeveloper.com"
    },
    "android": {
      "package": "www.hunchdeveloper.com"
    },
    "androidStatusBar": {
      "backgroundColor": "#00BCD4"
    }
  }
